						value matches {
							DV_PROPORTION matches {
								numerator matches {|0.0..5.0|}
								denominator matches {|100.0|}
								type matches {2}
							}
						}
					}
					ELEMENT[at0113] occurrences matches {0..1} matches {	-- Last zero offset calibration
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0114] occurrences matches {0..1} matches {	-- Data source platform
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0260,	-- Garmin Connect
									at0261,	-- TrainingPeaks
									at0262,	-- Strava
									at0263,	-- Zwift
									at0264,	-- Wahoo SYSTM/ELEMNT
									at0265,	-- Today's Plan
									at0266,	-- Golden Cheetah
									at0267]	-- Other
								}
							}
						}
					}
					allow_archetype CLUSTER[at0120] occurrences matches {0..*} matches {	-- Vendor data provenance
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.vendor_data_provenance\.v0/}
					}
				}
			}
		}
	}

