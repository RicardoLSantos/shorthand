archetype (adl_version=1.4)
    openEHR-EHR-OBSERVATION.hiking_activity.v0

concept
    [at0000]    -- Hiking activity

language
    original_language = <[ISO_639-1::en]>

description
    original_author = <
        ["name"] = <"Ricardo Correia">
        ["organisation"] = <"FMUP - Faculty of Medicine, University of Porto">
        ["email"] = <"correia@med.up.pt">
        ["date"] = <"2025-11-27">
    >
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record hiking and outdoor walking activities from wearable devices and GPS-enabled fitness trackers, including elevation, terrain, environmental conditions, and physiological metrics for lifestyle medicine applications.">
            use = <"Use to record:
- Hiking duration, distance, and elevation metrics
- Terrain type and trail difficulty
- GPS track data and route information
- Environmental conditions (altitude, weather, UV exposure)
- Physiological response (heart rate, calories, training load)
- Safety features (location sharing, emergency alerts)

This archetype supports data from:
- GPS watches (Garmin, Suunto, Coros, Polar)
- Smartwatches (Apple Watch, Samsung Galaxy Watch)
- Hiking-specific apps (AllTrails, Komoot, Gaia GPS)
- Altimeters and barometers in wearables

Designed for lifestyle medicine focusing on outdoor activity and nature exposure benefits.">
            keywords = <"hiking", "walking", "outdoor", "elevation", "trail", "GPS", "altitude", "wearable", "nature">
            misuse = <"Not to be used for:
- Indoor treadmill walking - use physical_activity_detailed archetype
- Running activities - use running_dynamics archetype
- Mountain climbing/technical climbing requiring specialized safety equipment">
            copyright = <"Â© openEHR Foundation, HEADS#2 FMUP">
        >
    >
    lifecycle_state = <"in_development">
    other_contributors = <"HEADS#2 PhD Project, FMUP", "openEHR Lifestyle Medicine Working Group">
    other_details = <
        ["licence"] = <"Creative Commons CC-BY-SA 4.0">
        ["custodian_organisation"] = <"openEHR Foundation">
        ["references"] = <"Bratman GN, et al. Nature and mental health: An ecosystem service perspective. Sci Adv. 2019;5(7):eaax0903.
Niedermeier M, et al. Acute effects of a short bout of physical activity in a natural environment on indices of mental health. BMC Public Health. 2017;17:1.
Ewert A, Chang Y. Levels of Nature and Stress Response. Behav Sci (Basel). 2018;8(5):49.">
        ["original_namespace"] = <"org.openehr">
        ["original_publisher"] = <"openEHR Foundation">
        ["custodian_namespace"] = <"org.openehr">
        ["MD5-CAM-1.0.1"] = <"hiking_activity_v0_md5">
        ["build_uid"] = <"hiking_activity_v0_build_001">
    >

definition
    OBSERVATION[at0000] matches {    -- Hiking activity
        data matches {
            HISTORY[at0001] matches {    -- History
                events cardinality matches {1..*; unordered} matches {
                    POINT_EVENT[at0002] occurrences matches {0..*} matches {    -- Any point in time event
                        data matches {
                            ITEM_TREE[at0003] matches {    -- Tree
                                items cardinality matches {0..*; unordered} matches {
                                    CLUSTER[at0010] occurrences matches {0..1} matches {    -- Activity summary
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0011] occurrences matches {0..1} matches {    -- Activity type
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0200,    -- Day hike
                                                            at0201,    -- Multi-day backpacking
                                                            at0202,    -- Trail walking
                                                            at0203,    -- Nordic walking
                                                            at0204,    -- Mountain hiking
                                                            at0205,    -- Snowshoeing
                                                            at0206,    -- Nature walk
                                                            at0207]    -- Urban hiking
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0012] occurrences matches {0..1} matches {    -- Total duration
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTHM/|PT0S..PT24H|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Moving time
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTHM/|PT0S..PT24H|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Stopped time
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTHM/|PT0S..PT12H|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0015] occurrences matches {0..1} matches {    -- Total distance
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::122]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"km">
                                                                magnitude = <|0.0..100.0|>
                                                                precision = <|2|>
                                                            >
                                                            ["2"] = <
                                                                units = <"[mi_i]">
                                                                magnitude = <|0.0..60.0|>
                                                                precision = <|2|>
                                                            >
                                                        >
