concept
	[at0000]	-- Fall detection event

language
	original_language = <[ISO_639-1::en]>

description
	original_author = <
		["name"] = <"Ricardo Lourenco dos Santos">
		["organisation"] = <"HEADS Lab - Faculty of Medicine University of Porto">
		["email"] = <"ricardo.lourenco@medfac.up.pt">
		["date"] = <"2025-11-27">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record fall detection events from wearable devices including event characteristics, user response, emergency actions, and fall risk tracking for elderly care and lifestyle medicine applications.">
			use = <"Use to record:
- Fall detection events from wearables (hard fall, soft fall, trip)
- Impact characteristics (magnitude, direction, duration)
- User response to fall alert (confirmed, dismissed, no response)
- Emergency actions triggered (SOS, emergency call, location shared)
- Pre-fall context (activity, location, time of day)
- Post-fall status and recovery
- Fall history and risk patterns
- False positive tracking for algorithm improvement

This archetype supports integration with:
- Apple Watch (Fall Detection feature)
- Samsung Galaxy Watch (Fall Detection)
- Garmin watches (Incident Detection)
- Google Pixel Watch (Fall Detection)
- Medical alert devices (Life Alert, Medical Guardian)
- Smart home fall detection systems

Intended for use in:
- Elderly care and aging in place programs
- Fall prevention research
- Post-fall monitoring
- Clinical fall risk assessment
- Emergency response coordination
- Lifestyle medicine for mobility.">
			misuse = <"Not to be used for clinical fall assessment scales - use appropriate clinical archetypes.
Not to be used as sole basis for clinical fall risk scoring.
Not to be used for post-fall injury assessment - use trauma/injury archetypes.
Device fall detection has limitations and may miss or falsely detect falls.">
			copyright = <"HEADS Lab - Faculty of Medicine University of Porto">
		>
	>
	lifecycle_state = <"in_development">
	other_contributors = <>
