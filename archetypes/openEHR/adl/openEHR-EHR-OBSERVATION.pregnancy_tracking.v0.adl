concept
    [at0000]    -- Pregnancy Tracking

language
    original_language = <[ISO_639-1::en]>

description
    original_author = <
        ["name"] = <"Ricardo Lourenco dos Santos">
        ["organisation"] = <"FMUP - Faculty of Medicine, University of Porto">
        ["email"] = <"ricardo.santos@med.up.pt">
        ["date"] = <"2025-11-27">
    >
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record pregnancy-related tracking data from consumer health applications, supporting self-monitoring during pregnancy and integration with prenatal care within lifestyle medicine frameworks.">
            use = <"Use to record pregnancy tracking data from consumer apps including:

Pregnancy progress:
- Gestational age (weeks and days)
- Trimester
- Due date (EDD)
- Days until due date

Self-monitoring:
- Weight tracking
- Blood pressure (if home monitoring)
- Kick counts
- Contraction timing

Symptoms tracking:
- Nausea/morning sickness
- Fatigue
- Mood changes
- Sleep quality
- Physical discomfort

Baby development:
- Size comparisons
- Milestone tracking
- Movement patterns

Wellness tracking:
- Prenatal vitamins
- Exercise
- Nutrition
- Hydration

Designed for integration with:
- Ovia Pregnancy
- The Bump
- What to Expect
- BabyCenter
- Pregnancy+
- Glow Nurture
- Apple Health pregnancy data
- Fitbit pregnancy mode

This archetype captures self-reported pregnancy tracking data from consumer applications. For clinical prenatal care documentation, obstetric assessments, or high-risk pregnancy monitoring, use appropriate clinical archetypes.">
            misuse = <"Not to be used for:
- Clinical prenatal care documentation
- Obstetric examination findings
- High-risk pregnancy monitoring
- Labor and delivery documentation
- Fetal assessment requiring clinical interpretation
- Gestational diabetes management protocols
- Pre-eclampsia monitoring requiring clinical oversight">
            keywords = <"pregnancy", "prenatal", "gestation", "trimester", "due date", "kick count", "contraction", "morning sickness", "baby", "fetal", "maternal", "wellness", "tracking", "lifestyle medicine">
            copyright = <"Â© openEHR Foundation, HEADS#2 PhD Project FMUP">
        >
    >
    lifecycle_state = <"in_development">
    other_contributors = <"HEADS#2 PhD Project, FMUP", "openEHR Community">
