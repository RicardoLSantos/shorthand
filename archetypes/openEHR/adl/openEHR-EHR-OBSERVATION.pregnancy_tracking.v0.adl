                                            ELEMENT[at0011] occurrences matches {0..1} matches {    -- Gestational weeks
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|0..45|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0012] occurrences matches {0..1} matches {    -- Gestational days
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|0..6|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Trimester
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0200,    -- First trimester
                                                            at0201,    -- Second trimester
                                                            at0202]    -- Third trimester
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Due date
                                                value matches {
