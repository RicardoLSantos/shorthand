archetype (adl_version=1.4)
    openEHR-EHR-OBSERVATION.yoga_session.v0

concept
    [at0000]    -- Yoga session

language
    original_language = <[ISO_639-1::en]>

description
    original_author = <
        ["name"] = <"Ricardo Correia">
        ["organisation"] = <"FMUP - Faculty of Medicine, University of Porto">
        ["email"] = <"correia@med.up.pt">
        ["date"] = <"2025-11-27">
    >
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record yoga practice sessions from wearable devices and fitness apps, including style, poses, breathwork, and physiological responses for lifestyle medicine applications.">
            use = <"Use to record:
- Yoga practice details (style, duration, intensity)
- Pose sequences and hold times
- Breathwork (pranayama) practices
- Heart rate and HRV during practice
- Mindfulness and meditation components
- Flexibility and balance metrics from wearables
- Energy expenditure and recovery effects

This archetype supports data from:
- Apple Watch yoga workouts
- Fitbit mindfulness sessions
- Garmin yoga activities
- Specialized yoga apps (Down Dog, Yoga Studio, Asana Rebel)
- Smart yoga mats with pose detection

Designed for lifestyle medicine focusing on mind-body practices and stress reduction.">
            keywords = <"yoga", "mindfulness", "asana", "pranayama", "flexibility", "meditation", "wearable", "wellness">
            misuse = <"Not to be used for:
- General meditation - use mental_wellness or mindfulness archetype
- Strength training - use strength_training archetype
- General physical activity - use physical_activity_detailed archetype">
            copyright = <"© openEHR Foundation, HEADS#2 FMUP">
        >
    >
    lifecycle_state = <"in_development">
    other_contributors = <"HEADS#2 PhD Project, FMUP", "openEHR Lifestyle Medicine Working Group">
    other_details = <
        ["licence"] = <"Creative Commons CC-BY-SA 4.0">
        ["custodian_organisation"] = <"openEHR Foundation">
        ["references"] = <"Pascoe MC, et al. Yoga, mindfulness-based stress reduction and stress-related physiological measures: A meta-analysis. Psychoneuroendocrinology. 2017;86:152-168.
Woodyard C. Exploring the therapeutic effects of yoga and its ability to increase quality of life. Int J Yoga. 2011;4(2):49-54.
Ross A, Thomas S. The health benefits of yoga and exercise: a review of comparison studies. J Altern Complement Med. 2010;16(1):3-12.">
        ["original_namespace"] = <"org.openehr">
        ["original_publisher"] = <"openEHR Foundation">
        ["custodian_namespace"] = <"org.openehr">
        ["MD5-CAM-1.0.1"] = <"yoga_session_v0_md5">
        ["build_uid"] = <"yoga_session_v0_build_001">
    >

definition
    OBSERVATION[at0000] matches {    -- Yoga session
        data matches {
            HISTORY[at0001] matches {    -- History
                events cardinality matches {1..*; unordered} matches {
                    POINT_EVENT[at0002] occurrences matches {0..*} matches {    -- Any point in time event
                        data matches {
                            ITEM_TREE[at0003] matches {    -- Tree
                                items cardinality matches {0..*; unordered} matches {
                                    CLUSTER[at0010] occurrences matches {0..1} matches {    -- Session details
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0011] occurrences matches {0..1} matches {    -- Yoga style
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0200,    -- Hatha
                                                            at0201,    -- Vinyasa/Flow
                                                            at0202,    -- Ashtanga
                                                            at0203,    -- Yin
                                                            at0204,    -- Restorative
                                                            at0205,    -- Power yoga
                                                            at0206,    -- Bikram/Hot yoga
                                                            at0207,    -- Iyengar
                                                            at0208,    -- Kundalini
                                                            at0209,    -- Prenatal
                                                            at0210,    -- Chair yoga
                                                            at0211,    -- Yoga Nidra
                                                            at0212]    -- Mixed/Other
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0012] occurrences matches {0..1} matches {    -- Session duration
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTHM/|PT0S..PT4H|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Intensity level
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0220,    -- Gentle/Beginner
                                                            at0221,    -- Moderate
                                                            at0222,    -- Vigorous
                                                            at0223]    -- Advanced/Intense
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Focus area
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0230,    -- Full body
                                                            at0231,    -- Core strength
                                                            at0232,    -- Hip opening
                                                            at0233,    -- Back/Spine
                                                            at0234,    -- Upper body
                                                            at0235,    -- Lower body
                                                            at0236,    -- Balance
                                                            at0237,    -- Flexibility
                                                            at0238,    -- Stress relief
                                                            at0239,    -- Energy/Morning
                                                            at0240]    -- Relaxation/Evening
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0015] occurrences matches {0..1} matches {    -- Guided session
                                                value matches {
                                                    DV_BOOLEAN matches {
                                                        value matches {True, False}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0016] occurrences matches {0..1} matches {    -- Instructor/App
                                                value matches {
                                                    DV_TEXT matches {*}
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0020] occurrences matches {0..1} matches {    -- Pose metrics
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0021] occurrences matches {0..1} matches {    -- Number of poses
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|0..100|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0022] occurrences matches {0..1} matches {    -- Total pose hold time
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTM/|PT0S..PT2H|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0023] occurrences matches {0..1} matches {    -- Average hold time per pose
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTS/|PT0S..PT10M|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0024] occurrences matches {0..1} matches {    -- Pose categories
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0250,    -- Standing poses
                                                            at0251,    -- Seated poses
                                                            at0252,    -- Supine poses
                                                            at0253,    -- Prone poses
                                                            at0254,    -- Inversions
                                                            at0255,    -- Arm balances
                                                            at0256,    -- Backbends
                                                            at0257,    -- Forward folds
                                                            at0258,    -- Twists
                                                            at0259]    -- Hip openers
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0025] occurrences matches {0..1} matches {    -- Pose accuracy score
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        denominator matches {|100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0030] occurrences matches {0..1} matches {    -- Breathwork (Pranayama)
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0031] occurrences matches {0..1} matches {    -- Pranayama included
                                                value matches {
                                                    DV_BOOLEAN matches {
                                                        value matches {True, False}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0032] occurrences matches {0..1} matches {    -- Breathwork duration
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTM/|PT0S..PT30M|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0033] occurrences matches {0..1} matches {    -- Breathing technique
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0260,    -- Ujjayi (Ocean breath)
                                                            at0261,    -- Kapalabhati (Breath of fire)
                                                            at0262,    -- Nadi Shodhana (Alternate nostril)
                                                            at0263,    -- Bhramari (Bee breath)
                                                            at0264,    -- Box breathing
                                                            at0265,    -- 4-7-8 breathing
                                                            at0266,    -- Diaphragmatic breathing
                                                            at0267]    -- Other
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0034] occurrences matches {0..1} matches {    -- Average breath rate during practice
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::382]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"/min">
                                                                magnitude = <|2.0..30.0|>
                                                                precision = <|1|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0040] occurrences matches {0..1} matches {    -- Physiological response
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0041] occurrences matches {0..1} matches {    -- Average heart rate
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::382]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"/min">
                                                                magnitude = <|30.0..220.0|>
                                                                precision = <|0|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0042] occurrences matches {0..1} matches {    -- Maximum heart rate
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::382]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"/min">
                                                                magnitude = <|30.0..220.0|>
                                                                precision = <|0|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0043] occurrences matches {0..1} matches {    -- Minimum heart rate
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::382]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"/min">
                                                                magnitude = <|30.0..220.0|>
                                                                precision = <|0|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0044] occurrences matches {0..1} matches {    -- HRV during session (RMSSD)
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::128]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms">
                                                                magnitude = <|0.0..300.0|>
                                                                precision = <|1|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0045] occurrences matches {0..1} matches {    -- Calories burned
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::121]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"kcal">
                                                                magnitude = <|0.0..2000.0|>
                                                                precision = <|0|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0046] occurrences matches {0..1} matches {    -- Heart rate zone time
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0270,    -- Mostly resting (<50% max HR)
                                                            at0271,    -- Mostly light (50-60% max HR)
                                                            at0272,    -- Mixed (light-moderate)
                                                            at0273]    -- Moderate intensity (60-70% max HR)
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0050] occurrences matches {0..1} matches {    -- Meditation/Savasana
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0051] occurrences matches {0..1} matches {    -- Meditation included
                                                value matches {
                                                    DV_BOOLEAN matches {
                                                        value matches {True, False}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0052] occurrences matches {0..1} matches {    -- Meditation duration
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTM/|PT0S..PT60M|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0053] occurrences matches {0..1} matches {    -- Savasana duration
                                                value matches {
                                                    DV_DURATION matches {
                                                        value matches {PTM/|PT0S..PT20M|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0054] occurrences matches {0..1} matches {    -- Mindfulness score
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::380]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"1">
                                                                magnitude = <|0.0..100.0|>
                                                                precision = <|0|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0060] occurrences matches {0..1} matches {    -- Subjective assessment
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0061] occurrences matches {0..1} matches {    -- Pre-session stress level
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|1..10|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0062] occurrences matches {0..1} matches {    -- Post-session stress level
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|1..10|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0063] occurrences matches {0..1} matches {    -- Session satisfaction
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|1..5|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0064] occurrences matches {0..1} matches {    -- Perceived difficulty
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0280,    -- Too easy
                                                            at0281,    -- Just right
                                                            at0282,    -- Challenging
                                                            at0283]    -- Too difficult
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0065] occurrences matches {0..1} matches {    -- Session notes
                                                value matches {
                                                    DV_TEXT matches {*}
                                                }
                                            }
                                        }
                                    }
                                    allow_archetype CLUSTER[at0080] occurrences matches {0..*} matches {    -- Wearable device
                                        include
                                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.wearable_device\.v0|openEHR-EHR-CLUSTER\.device\.v1/}
                                    }
                                    allow_archetype CLUSTER[at0081] occurrences matches {0..*} matches {    -- Data quality indicator
                                        include
                                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.data_quality_indicator\.v0/}
                                    }
                                }
                            }
                        }
                        state matches {
                            ITEM_TREE[at0090] matches {    -- State
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[at0091] occurrences matches {0..1} matches {    -- Environment
                                        value matches {
                                            DV_CODED_TEXT matches {
                                                defining_code matches {
                                                    [local::
                                                    at0290,    -- Home
                                                    at0291,    -- Yoga studio
                                                    at0292,    -- Gym
                                                    at0293,    -- Outdoors
                                                    at0294]    -- Other
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0092] occurrences matches {0..1} matches {    -- Room temperature
                                        value matches {
                                            C_DV_QUANTITY <
                                                property = <[openehr::127]>
                                                list = <
                                                    ["1"] = <
                                                        units = <"Cel">
                                                        magnitude = <|15.0..45.0|>
                                                        precision = <|1|>
                                                    >
                                                >
                                            >
                                        }
                                    }
                                    ELEMENT[at0093] occurrences matches {0..1} matches {    -- Props used
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                }
                            }
                        }
                    }
                    INTERVAL_EVENT[at0100] occurrences matches {0..*} matches {    -- Weekly summary
                        math_function matches {
                            DV_CODED_TEXT matches {
                                defining_code matches {
                                    [openehr::148]
                                }
                            }
                        }
                        width matches {
                            DV_DURATION matches {
                                value matches {P1W}
                            }
                        }
                        data matches {
                            use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
                        }
                        state matches {
                            use_node ITEM_TREE /data[at0001]/events[at0002]/state[at0090]
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[at0110] matches {    -- Protocol
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[at0111] occurrences matches {0..1} matches {    -- Data source
                        value matches {
                            DV_CODED_TEXT matches {
                                defining_code matches {
                                    [local::
