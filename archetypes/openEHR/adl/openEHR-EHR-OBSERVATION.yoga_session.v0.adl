                                    at0302,    -- Yoga Studio
                                    at0303,    -- Asana Rebel
                                    at0304,    -- Peloton
                                    at0305,    -- Glo
                                    at0306,    -- Alo Moves
                                    at0307,    -- Garmin Connect
                                    at0308,    -- Fitbit
                                    at0309,    -- Manual entry
                                    at0310]    -- Other
                                }
                            }
                        }
                    }
                    ELEMENT[at0112] occurrences matches {0..1} matches {    -- Smart mat used
                        value matches {
                            DV_BOOLEAN matches {
                                value matches {True, False}
                            }
                        }
                    }
                    allow_archetype CLUSTER[at0120] occurrences matches {0..*} matches {    -- Vendor data provenance
                        include
                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.vendor_data_provenance\.v0/}
                    }
                }
            }
        }
    }

