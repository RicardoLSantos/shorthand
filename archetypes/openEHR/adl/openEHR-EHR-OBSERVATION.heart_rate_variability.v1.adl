definition
    OBSERVATION[id1] matches {    -- Heart rate variability
        data matches {
            HISTORY[id2] matches {
                events cardinality matches {1..*; unordered} matches {
                    EVENT[id3] occurrences matches {0..*} matches {    -- Any event
                        data matches {
                            ITEM_TREE[id4] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    -- TIME DOMAIN METRICS
                                    ELEMENT[id5] occurrences matches {0..1} matches {    -- SDNN
                                        value matches {
                                            DV_QUANTITY[id100] matches {
                                                property matches {[openehr::128]}    -- Time
                                                units matches {"ms"}
                                                magnitude matches {|0.0..500.0|}
                                                precision matches {1}
                                            }
                                        }
                                    }
                                    ELEMENT[id6] occurrences matches {0..1} matches {    -- RMSSD
                                        value matches {
                                            DV_QUANTITY[id101] matches {
                                                property matches {[openehr::128]}
                                                units matches {"ms"}
                                                magnitude matches {|0.0..300.0|}
                                                precision matches {1}
                                            }
                                        }
                                    }
                                    ELEMENT[id7] occurrences matches {0..1} matches {    -- pNN50
                                        value matches {
                                            DV_PROPORTION[id102] matches {
                                                numerator matches {|0.0..100.0|}
                                                type matches {2}    -- percent
                                            }
                                        }
                                    }
                                    ELEMENT[id8] occurrences matches {0..1} matches {    -- SDANN
                                        value matches {
                                            DV_QUANTITY[id103] matches {
                                                property matches {[openehr::128]}
                                                units matches {"ms"}
                                                magnitude matches {|0.0..500.0|}
                                                precision matches {1}
                                            }
                                        }
                                    }
                                    -- FREQUENCY DOMAIN METRICS
                                    CLUSTER[id10] occurrences matches {0..1} matches {    -- Spectral analysis
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[id11] occurrences matches {0..1} matches {    -- LF Power
                                                value matches {
                                                    DV_QUANTITY[id110] matches {
                                                        property matches {[openehr::380]}    -- Power
                                                        units matches {"ms2"}
                                                        magnitude matches {|0.0..10000.0|}
                                                        precision matches {1}
                                                    }
                                                }
                                            }
                                            ELEMENT[id12] occurrences matches {0..1} matches {    -- HF Power
                                                value matches {
                                                    DV_QUANTITY[id111] matches {
                                                        property matches {[openehr::380]}
                                                        units matches {"ms2"}
                                                        magnitude matches {|0.0..10000.0|}
                                                        precision matches {1}
                                                    }
                                                }
                                            }
                                            ELEMENT[id13] occurrences matches {0..1} matches {    -- LF/HF Ratio
                                                value matches {
                                                    DV_QUANTITY[id112] matches {
                                                        property matches {[openehr::380]}
                                                        units matches {"1"}    -- dimensionless
                                                        magnitude matches {|0.0..10.0|}
                                                        precision matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[id14] occurrences matches {0..1} matches {    -- VLF Power
                                                value matches {
                                                    DV_QUANTITY[id113] matches {
                                                        property matches {[openehr::380]}
                                                        units matches {"ms2"}
                                                        magnitude matches {|0.0..10000.0|}
                                                        precision matches {1}
                                                    }
                                                }
                                            }
                                            ELEMENT[id15] occurrences matches {0..1} matches {    -- Total Power
                                                value matches {
                                                    DV_QUANTITY[id114] matches {
                                                        property matches {[openehr::380]}
                                                        units matches {"ms2"}
                                                        magnitude matches {|0.0..50000.0|}
                                                        precision matches {1}
                                                    }
                                                }
                                            }
                                            ELEMENT[id16] occurrences matches {0..1} matches {    -- Spectral method
                                                value matches {
                                                    DV_CODED_TEXT[id115] matches {
                                                        defining_code matches {
                                                            [local::
                                                            at50,    -- FFT
                                                            at51,    -- Autoregressive
                                                            at52,    -- Welch periodogram
                                                            at53]    -- Lomb-Scargle
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    -- NONLINEAR METRICS (POINCARE)
                                    CLUSTER[id20] occurrences matches {0..1} matches {    -- Poincare plot analysis
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[id21] occurrences matches {0..1} matches {    -- SD1
                                                value matches {
                                                    DV_QUANTITY[id120] matches {
                                                        property matches {[openehr::128]}
                                                        units matches {"ms"}
                                                        magnitude matches {|0.0..200.0|}
                                                        precision matches {1}
                                                    }
                                                }
                                            }
                                            ELEMENT[id22] occurrences matches {0..1} matches {    -- SD2
                                                value matches {
                                                    DV_QUANTITY[id121] matches {
                                                        property matches {[openehr::128]}
                                                        units matches {"ms"}
                                                        magnitude matches {|0.0..500.0|}
                                                        precision matches {1}
                                                    }
                                                }
                                            }
                                            ELEMENT[id23] occurrences matches {0..1} matches {    -- SD1/SD2 Ratio
                                                value matches {
                                                    DV_QUANTITY[id122] matches {
                                                        property matches {[openehr::380]}
                                                        units matches {"1"}
                                                        magnitude matches {|0.0..5.0|}
                                                        precision matches {3}
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    -- METADATA
                                    ELEMENT[id30] occurrences matches {0..1} matches {    -- Number of NN intervals
                                        value matches {
                                            DV_COUNT[id130] matches {
                                                magnitude matches {|0..200000|}
                                            }
                                        }
                                    }
                                    ELEMENT[id31] occurrences matches {0..1} matches {    -- Ectopic beat count
                                        value matches {
                                            DV_COUNT[id131] matches {
                                                magnitude matches {|0..10000|}
                                            }
                                        }
                                    }
                                    ELEMENT[id32] occurrences matches {0..1} matches {    -- Recording duration
                                        value matches {
                                            DV_DURATION[id132] matches {
                                                value matches {PTHMS/|PT1M..PT24H|}
                                            }
                                        }
                                    }
                                    ELEMENT[id33] occurrences matches {0..1} matches {    -- Clinical interpretation
                                        value matches {
                                            DV_TEXT[id133]
                                        }
                                    }
                                    ELEMENT[id34] occurrences matches {0..1} matches {    -- Comment
                                        value matches {
