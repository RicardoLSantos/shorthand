                                                    at80,    -- Spontaneous
                                                    at81,    -- Paced 12 breaths/min
                                                    at82,    -- Paced 15 breaths/min
                                                    at83]    -- Guided breathing app
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id44] occurrences matches {0..1} matches {    -- Confounding factors
                                        value matches {
                                            DV_TEXT[id143]
                                        }
                                    }
                                    allow_archetype CLUSTER[id45] occurrences matches {0..*} matches {    -- Level of exertion
                                        include
                                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.level_of_exertion\.v.*/}
                                    }
