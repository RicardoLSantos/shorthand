                                                defining_code matches {
                                                    [local::
                                                    at60,    -- Resting (sitting)
                                                    at61,    -- Resting (supine)
                                                    at62,    -- Active
                                                    at63,    -- Sleep
                                                    at64,    -- Post-exercise recovery
                                                    at65]    -- Paced breathing
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[id42] occurrences matches {0..1} matches {    -- Body position
                                        value matches {
                                            DV_CODED_TEXT[id141] matches {
                                                defining_code matches {
                                                    [local::
                                                    at70,    -- Sitting
                                                    at71,    -- Standing
                                                    at72,    -- Lying supine
                                                    at73,    -- Lying prone
                                                    at74,    -- Lying left lateral
                                                    at75]    -- Lying right lateral
