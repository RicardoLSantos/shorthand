    [at0000]    -- Fertility Indicators

language
    original_language = <[ISO_639-1::en]>

description
    original_author = <
        ["name"] = <"Ricardo Lourenco dos Santos">
        ["organisation"] = <"FMUP - Faculty of Medicine, University of Porto">
        ["email"] = <"ricardo.santos@med.up.pt">
        ["date"] = <"2025-11-27">
    >
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record fertility-related biomarkers and indicators from consumer health devices and applications, supporting fertility awareness methods (FAM), conception planning, and natural family planning within lifestyle medicine frameworks.">
            use = <"Use to record fertility indicator data from consumer devices including:

Basal body temperature (BBT):
- Morning temperature measurements
- Temperature shift detection
- Thermal shift confirmation
- Cover line calculation

Cervical mucus observations:
- Mucus presence and quantity
- Mucus quality/type (dry, sticky, creamy, egg white)
- Peak day identification
- Mucus pattern tracking

Ovulation testing:
- LH test results (hormone strips)
- LH surge detection
- Digital ovulation test results
- Progesterone confirmation tests

Cervical position (optional):
- Height (low/medium/high)
- Firmness (firm/medium/soft)
- Opening (closed/partially open/open)

Fertility signs synthesis:
- Fertile window status
- Ovulation confirmed/predicted
- Peak fertility day
- Days post-ovulation (DPO)

Designed for integration with:
- Natural Cycles (FDA-cleared)
- Tempdrop wearable
- Ava Fertility bracelet
- Mira Fertility analyzer
- Clearblue Connected
- Kindara
- Fertility Friend
- Kegg cervical mucus tracker
- Modern Fertility

This archetype captures self-tracked fertility biomarkers from consumer devices. For clinical fertility assessment or assisted reproduction protocols, use appropriate clinical archetypes.">
            misuse = <"Not to be used for:
- Clinical infertility diagnosis
- IVF or assisted reproduction protocols
- Clinical hormone level interpretation
- Medical fertility treatment monitoring
- Pregnancy confirmation (use pregnancy archetype)
- Clinical cervical examination findings">
            keywords = <"fertility", "BBT", "basal temperature", "ovulation", "LH", "cervical mucus", "fertile window", "conception", "FAM", "NFP", "natural family planning", "TTC", "trying to conceive", "wearable", "lifestyle medicine">
            copyright = <"Â© openEHR Foundation, HEADS#2 PhD Project FMUP">
        >
    >
    lifecycle_state = <"in_development">
