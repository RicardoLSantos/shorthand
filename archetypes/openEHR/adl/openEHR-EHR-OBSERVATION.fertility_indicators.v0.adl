definition
    OBSERVATION[at0000] matches {    -- Fertility Indicators
        data matches {
            HISTORY[at0001] matches {    -- History
                events cardinality matches {1..*; unordered} matches {
                    EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
                        data matches {
                            ITEM_TREE[at0003] matches {    -- Tree
                                items cardinality matches {0..*; unordered} matches {
                                    CLUSTER[at0010] occurrences matches {0..1} matches {    -- Basal body temperature
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0011] occurrences matches {0..1} matches {    -- BBT value
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::127]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"Cel">
                                                                magnitude = <|35.0..40.0|>
                                                                precision = <|2|>
                                                            >
                                                            ["2"] = <
                                                                units = <"[degF]">
                                                                magnitude = <|95.0..104.0|>
                                                                precision = <|2|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0012] occurrences matches {0..1} matches {    -- Measurement time
                                                value matches {
                                                    DV_TIME matches {*}
                                                }
                                            }
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Temperature phase
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0200,    -- Pre-ovulatory (low)
                                                            at0201,    -- Thermal shift
                                                            at0202,    -- Post-ovulatory (high)
                                                            at0203]    -- Uncertain
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Cover line
                                                value matches {
                                                    C_DV_QUANTITY <
                                                        property = <[openehr::127]>
                                                        list = <
                                                            ["1"] = <
                                                                units = <"Cel">
                                                                magnitude = <|35.0..40.0|>
                                                                precision = <|2|>
                                                            >
                                                        >
                                                    >
                                                }
                                            }
                                            ELEMENT[at0015] occurrences matches {0..1} matches {    -- Days of high temps
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|0..20|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0016] occurrences matches {0..1} matches {    -- Disturbed sleep
                                                value matches {
                                                    DV_BOOLEAN matches {*}
                                                }
                                            }
                                            ELEMENT[at0017] occurrences matches {0..1} matches {    -- Disturbance factors
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0210,    -- None
                                                            at0211,    -- Poor sleep
                                                            at0212,    -- Alcohol
                                                            at0213,    -- Illness/fever
                                                            at0214,    -- Travel/time zone
                                                            at0215,    -- Late measurement
                                                            at0216]    -- Other
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0020] occurrences matches {0..1} matches {    -- Cervical mucus
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0021] occurrences matches {0..1} matches {    -- Mucus presence
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0220,    -- None/dry
                                                            at0221,    -- Minimal
                                                            at0222,    -- Moderate
                                                            at0223]    -- Abundant
                                                        }
                                                    }
