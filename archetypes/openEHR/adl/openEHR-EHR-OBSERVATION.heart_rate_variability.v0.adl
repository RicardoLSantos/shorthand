definition
    OBSERVATION[at0000] matches {    -- Heart rate variability
        data matches {
            HISTORY[at0001] matches {
                events cardinality matches {1..*; unordered} matches {
                    EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
                        data matches {
                            ITEM_TREE[at0003] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[at0004] occurrences matches {0..1} matches {    -- SDNN
                                        value matches {
                                            DV_QUANTITY matches {
                                                property matches {[openehr::128]}
                                                list = <
                                                    ["1"] = <
                                                        units = <"ms">
                                                        magnitude = <|0.0..500.0|>
                                                        precision = <|1|>
                                                    >
                                                >
                                            }
                                        }
                                    }
                                    ELEMENT[at0005] occurrences matches {0..1} matches {    -- RMSSD
                                        value matches {
                                            DV_QUANTITY matches {
                                                property matches {[openehr::128]}
                                                list = <
                                                    ["1"] = <
                                                        units = <"ms">
                                                        magnitude = <|0.0..300.0|>
                                                        precision = <|1|>
                                                    >
                                                >
                                            }
                                        }
                                    }
                                    ELEMENT[at0006] occurrences matches {0..1} matches {    -- pNN50
                                        value matches {
                                            DV_PROPORTION matches {
                                                numerator matches {|0.0..100.0|}
                                                type matches {2}
                                            }
                                        }
                                    }
                                    ELEMENT[at0007] occurrences matches {0..1} matches {    -- SDANN
                                        value matches {
                                            DV_QUANTITY matches {
                                                property matches {[openehr::128]}
                                                list = <
                                                    ["1"] = <
                                                        units = <"ms">
                                                        magnitude = <|0.0..500.0|>
                                                        precision = <|1|>
                                                    >
                                                >
                                            }
                                        }
                                    }
                                    CLUSTER[at0008] occurrences matches {0..1} matches {    -- Spectral analysis
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0009] occurrences matches {0..1} matches {    -- LF Power
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::380]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms2">
                                                                magnitude = <|0.0..10000.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0010] occurrences matches {0..1} matches {    -- HF Power
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::380]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms2">
                                                                magnitude = <|0.0..10000.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0011] occurrences matches {0..1} matches {    -- LF/HF Ratio
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::380]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"1">
                                                                magnitude = <|0.0..10.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0012] occurrences matches {0..1} matches {    -- VLF Power
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::380]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms2">
                                                                magnitude = <|0.0..10000.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Total Power
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::380]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms2">
                                                                magnitude = <|0.0..50000.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Spectral method
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0050,    -- FFT
                                                            at0051,    -- Autoregressive
                                                            at0052,    -- Welch periodogram
                                                            at0053]    -- Lomb-Scargle
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0015] occurrences matches {0..1} matches {    -- Poincare plot analysis
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0016] occurrences matches {0..1} matches {    -- SD1
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::128]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms">
                                                                magnitude = <|0.0..200.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0017] occurrences matches {0..1} matches {    -- SD2
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::128]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"ms">
                                                                magnitude = <|0.0..500.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                            ELEMENT[at0018] occurrences matches {0..1} matches {    -- SD1/SD2 Ratio
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::380]}
                                                        list = <
                                                            ["1"] = <
                                                                units = <"1">
                                                                magnitude = <|0.0..5.0|>
                                                            >
                                                        >
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0019] occurrences matches {0..1} matches {    -- Number of NN intervals
                                        value matches {
                                            DV_COUNT matches {
                                                magnitude matches {|0..200000|}
                                            }
                                        }
                                    }
                                    ELEMENT[at0020] occurrences matches {0..1} matches {    -- Ectopic beat count
                                        value matches {
                                            DV_COUNT matches {
                                                magnitude matches {|0..10000|}
                                            }
                                        }
                                    }
                                    ELEMENT[at0021] occurrences matches {0..1} matches {    -- Recording duration
                                        value matches {
                                            DV_DURATION matches {*}
                                        }
                                    }
                                    ELEMENT[at0022] occurrences matches {0..1} matches {    -- Clinical interpretation
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                    ELEMENT[at0023] occurrences matches {0..1} matches {    -- Comment
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                }
                            }
                        }
                        state matches {
                            ITEM_TREE[at0024] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[at0025] occurrences matches {0..1} matches {    -- Physiological state
                                        value matches {
                                            DV_CODED_TEXT matches {
                                                defining_code matches {
                                                    [local::
                                                    at0060,    -- Resting (sitting)
                                                    at0061,    -- Resting (supine)
                                                    at0062,    -- Active
                                                    at0063,    -- Sleep
                                                    at0064,    -- Post-exercise recovery
                                                    at0065]    -- Paced breathing
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0026] occurrences matches {0..1} matches {    -- Body position
                                        value matches {
                                            DV_CODED_TEXT matches {
                                                defining_code matches {
                                                    [local::
