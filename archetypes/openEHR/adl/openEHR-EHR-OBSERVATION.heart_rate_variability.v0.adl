                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0036] occurrences matches {0..1} matches {    -- Software version
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    allow_archetype CLUSTER[at0037] occurrences matches {0..*} matches {
                        include
                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.device\.v.*/}
                    }
                    allow_archetype CLUSTER[at0038] occurrences matches {0..*} matches {
                        include
