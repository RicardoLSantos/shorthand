                                                        [local::
                                                        at230,    -- Resting
                                                        at231,    -- Sleep
                                                        at232,    -- Light activity
                                                        at233,    -- Moderate activity
                                                        at234,    -- Vigorous activity
                                                        at235,    -- Post-exercise recovery
