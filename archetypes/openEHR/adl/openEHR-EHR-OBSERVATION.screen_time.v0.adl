archetype (adl_version=1.4)
    openEHR-EHR-OBSERVATION.screen_time.v0

concept
    [at0000]    -- Screen time

language
    original_language = <[ISO_639-1::en]>

description
    original_author = <
        ["name"] = <"Ricardo Correia">
        ["organisation"] = <"FMUP - Faculty of Medicine, University of Porto">
        ["email"] = <"correia@med.up.pt">
        ["date"] = <"2025-11-27">
    >
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record screen time and digital device usage patterns from smartphones, tablets, and computers for digital wellness assessment in lifestyle medicine applications.">
            use = <"Use to record:
- Total daily screen time across devices
- App category usage breakdown (social media, entertainment, productivity)
- Device pickups and notifications
- Focus time and downtime periods
- Bedtime and night-time device usage
- Blue light filter usage
- Digital wellbeing scores from device platforms

This archetype supports data from:
- Apple Screen Time (iOS/macOS)
- Google Digital Wellbeing (Android)
- Samsung Digital Wellbeing
- Third-party apps (RescueTime, Moment, Forest)
- Computer usage tracking software

Designed for lifestyle medicine focusing on digital wellness and its impact on sleep, mental health, and social connections.">
            keywords = <"screen time", "digital wellness", "smartphone", "social media", "notifications", "blue light", "sleep hygiene">
            misuse = <"Not to be used for:
- Clinical assessment of technology addiction - requires specialized psychological evaluation
- Workplace productivity monitoring without consent
- Child monitoring without appropriate safeguards">
            copyright = <"Â© openEHR Foundation, HEADS#2 FMUP">
        >
    >
    lifecycle_state = <"in_development">
    other_contributors = <"HEADS#2 PhD Project, FMUP", "openEHR Lifestyle Medicine Working Group">
    other_details = <
        ["licence"] = <"Creative Commons CC-BY-SA 4.0">
        ["custodian_organisation"] = <"openEHR Foundation">
        ["references"] = <"Twenge JM, et al. Increases in Depressive Symptoms, Suicide-Related Outcomes, and Suicide Rates Among U.S. Adolescents After 2010. Clin Psychol Sci. 2018;6(1):3-17.
Christensen MA, et al. Direct Measurements of Smartphone Screen-Time: Relationships with Demographics and Sleep. PLoS One. 2016;11(11):e0165331.
WHO guidelines on physical activity, sedentary behaviour and sleep for children under 5 years of age. WHO; 2019.">
        ["original_namespace"] = <"org.openehr">
        ["original_publisher"] = <"openEHR Foundation">
        ["custodian_namespace"] = <"org.openehr">
        ["MD5-CAM-1.0.1"] = <"screen_time_v0_md5">
        ["build_uid"] = <"screen_time_v0_build_001">
    >

definition
    OBSERVATION[at0000] matches {    -- Screen time
        data matches {
            HISTORY[at0001] matches {    -- History
                events cardinality matches {1..*; unordered} matches {
