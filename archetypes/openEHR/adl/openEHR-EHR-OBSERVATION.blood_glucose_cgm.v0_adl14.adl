language
    original_language = <[ISO_639-1::en]>

description
    original_author = <
        ["name"] = <"Ricardo Lourenco dos Santos">
        ["organisation"] = <"Faculty of Medicine, University of Porto (FMUP)">
        ["email"] = <"ricardolourencosantos@gmail.com">
        ["date"] = <"2025-11-27">
    >
    lifecycle_state = <"in_development">
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record continuous glucose monitoring (CGM) data from consumer and medical-grade wearable glucose sensors, including real-time glucose values, trend information, time-in-range metrics, and glycemic variability indices for diabetes management and metabolic health optimization.">
            use = <"Use to record CGM data including:

1. Real-time Glucose: Current interstitial glucose readings
2. Trend Data: Direction and rate of glucose change
3. Time in Range (TIR): Percentage of time in target glucose range
4. Glycemic Variability: CV, SD, MAGE, and other variability metrics
5. Events: Hypoglycemia and hyperglycemia episodes

CGM Device Categories:
- Medical-grade: Dexcom G6/G7, Abbott FreeStyle Libre, Medtronic Guardian
- Consumer wellness: Levels, Nutrisense, Supersapiens, Veri
- Research: continuous glucose-ketone monitors

Key Clinical Applications:
- Type 1 and Type 2 diabetes management
- Gestational diabetes monitoring
- Metabolic health optimization (non-diabetic)
- Dietary response assessment
- Athletic performance optimization
- Circadian glucose pattern analysis

Standardized Metrics (International Consensus 2019):
- Time in Range (TIR): 70-180 mg/dL (3.9-10.0 mmol/L) target >70%
- Time Below Range (TBR): <70 mg/dL target <4%
- Time Above Range (TAR): >180 mg/dL target <25%
- Glucose Management Indicator (GMI): estimated HbA1c
- Coefficient of Variation (CV): target <36%

Important Notes:
- CGM measures interstitial glucose, not blood glucose
- 5-15 minute lag behind blood glucose
- Calibration requirements vary by device
- Consumer CGM typically less accurate than medical-grade
- Sensor warm-up period affects initial readings">
            keywords = <"CGM", "continuous glucose monitoring", "glucose", "blood sugar", "diabetes", "interstitial glucose", "time in range", "TIR", "glycemic variability", "Dexcom", "FreeStyle Libre", "hypoglycemia", "hyperglycemia", "GMI", "metabolic health">
            misuse = <"Not to be used for:
- Point-of-care blood glucose measurements (use OBSERVATION.laboratory_test_result)
- HbA1c laboratory results
- Insulin pump data (use separate archetype)
- Ketone monitoring (separate archetype)
- Diagnostic decisions without confirmatory blood glucose">
            copyright = <"CC BY-SA 4.0">
        >
    >
    other_details = <
        ["licence"] = <"Creative Commons Attribution-ShareAlike 4.0 International (CC BY-SA 4.0)">
        ["custodian_organisation"] = <"FMUP - Faculty of Medicine, University of Porto">
        ["references"] = <"
1. LOINC 41653-7: Glucose [Mass/volume] in Capillary blood by Glucometer
2. LOINC 2339-0: Glucose [Mass/volume] in Blood
3. Battelino T, et al. Clinical Targets for Continuous Glucose Monitoring Data Interpretation: Recommendations From the International Consensus on Time in Range. Diabetes Care. 2019;42(8):1593-1603.
4. Danne T, et al. International Consensus on Use of Continuous Glucose Monitoring. Diabetes Care. 2017;40(12):1631-1640.
5. Beck RW, et al. Validation of Time in Range as an Outcome Measure for Diabetes Clinical Trials. Diabetes Care. 2019;42(3):400-405.">
        ["current_contact"] = <"Ricardo Lourenco dos Santos, FMUP, ricardolourencosantos@gmail.com">
        ["links"] = <"https://linktr.ee/ricardolsantos">
    >

definition
    OBSERVATION[at0000] matches {    -- Blood Glucose CGM
        data matches {
            HISTORY[at0001] matches {
                events cardinality matches {1..*; unordered} matches {
                    EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
                        data matches {
                            ITEM_TREE[at0003] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[at0004] occurrences matches {0..1} matches {    -- Glucose value
                                        value matches {
                                            DV_QUANTITY matches {
                                                property matches {[openehr::119]}
                                                units matches {"mg/dL", "mmol/L"}
                                                magnitude matches {|0.0..600.0|}
                                                precision matches {|0|}
                                            }
                                        }
                                    }
                                    ELEMENT[at0005] occurrences matches {0..1} matches {    -- Glucose interpretation
                                        value matches {
                                            DV_CODED_TEXT matches {
                                                defining_code matches {
                                                    [local::
                                                    at200,    -- Severe hypoglycemia (<54 mg/dL)
                                                    at201,    -- Hypoglycemia (54-69 mg/dL)
                                                    at202,    -- In range (70-180 mg/dL)
                                                    at203,    -- Hyperglycemia (181-250 mg/dL)
                                                    at204,    -- Severe hyperglycemia (>250 mg/dL)
                                                    at205]    -- Unknown
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0006] occurrences matches {0..1} matches {    -- Trend arrow
                                        value matches {
                                            DV_CODED_TEXT matches {
                                                defining_code matches {
                                                    [local::
                                                    at210,    -- Falling rapidly (>3 mg/dL/min)
                                                    at211,    -- Falling (2-3 mg/dL/min)
                                                    at212,    -- Falling slowly (1-2 mg/dL/min)
                                                    at213,    -- Stable (<1 mg/dL/min)
                                                    at214,    -- Rising slowly (1-2 mg/dL/min)
                                                    at215,    -- Rising (2-3 mg/dL/min)
                                                    at216,    -- Rising rapidly (>3 mg/dL/min)
                                                    at217]    -- Unknown
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0007] occurrences matches {0..1} matches {    -- Rate of change
                                        value matches {
                                            DV_QUANTITY matches {
                                                property matches {[openehr::341]}
                                                units matches {"mg/dL/min", "mmol/L/min"}
                                                magnitude matches {|-10.0..10.0|}
                                                precision matches {|2|}
                                            }
                                        }
                                    }
                                    CLUSTER[at0010] occurrences matches {0..1} matches {    -- Time in Range metrics
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0011] occurrences matches {0..1} matches {    -- Time in Range (TIR)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0012] occurrences matches {0..1} matches {    -- Time Below Range (TBR)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Time Below Range - Level 2 (<54)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Time Above Range (TAR)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0015] occurrences matches {0..1} matches {    -- Time Above Range - Level 2 (>250)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0016] occurrences matches {0..1} matches {    -- TIR target range lower
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::119]}
                                                        units matches {"mg/dL", "mmol/L"}
                                                        magnitude matches {|50.0..100.0|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0017] occurrences matches {0..1} matches {    -- TIR target range upper
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::119]}
                                                        units matches {"mg/dL", "mmol/L"}
                                                        magnitude matches {|100.0..250.0|}
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0020] occurrences matches {0..1} matches {    -- Glycemic variability
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0021] occurrences matches {0..1} matches {    -- Mean glucose
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::119]}
                                                        units matches {"mg/dL", "mmol/L"}
                                                        magnitude matches {|0.0..400.0|}
                                                        precision matches {|1|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0022] occurrences matches {0..1} matches {    -- Standard deviation
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::119]}
                                                        units matches {"mg/dL", "mmol/L"}
                                                        magnitude matches {|0.0..150.0|}
                                                        precision matches {|1|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0023] occurrences matches {0..1} matches {    -- Coefficient of variation (CV)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|0.0..100.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0024] occurrences matches {0..1} matches {    -- CV interpretation
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at220,    -- Stable (<36%)
                                                            at221,    -- Unstable (>=36%)
