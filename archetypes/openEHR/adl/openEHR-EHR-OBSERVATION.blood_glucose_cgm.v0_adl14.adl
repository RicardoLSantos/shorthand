                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0025] occurrences matches {0..1} matches {    -- Glucose Management Indicator (GMI)
                                                value matches {
                                                    DV_PROPORTION matches {
                                                        numerator matches {|4.0..15.0|}
                                                        type matches {2}
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0030] occurrences matches {0..1} matches {    -- Hypoglycemia events
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0031] occurrences matches {0..1} matches {    -- Number of events
                                                value matches {
                                                    DV_COUNT matches {
                                                        magnitude matches {|>=0|}
                                                    }
                                                }
                                            }
                                            ELEMENT[at0032] occurrences matches {0..1} matches {    -- Total duration
                                                value matches {
                                                    DV_DURATION matches {*}
                                                }
                                            }
                                            ELEMENT[at0033] occurrences matches {0..1} matches {    -- Lowest glucose
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::119]}
                                                        units matches {"mg/dL", "mmol/L"}
                                                        magnitude matches {|0.0..70.0|}
                                                    }
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0035] occurrences matches {0..1} matches {    -- Hyperglycemia events
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0036] occurrences matches {0..1} matches {    -- Number of events
                                                value matches {
