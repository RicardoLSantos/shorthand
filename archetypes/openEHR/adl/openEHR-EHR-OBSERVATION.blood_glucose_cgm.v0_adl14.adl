                                            ELEMENT[at0038] occurrences matches {0..1} matches {    -- Highest glucose
                                                value matches {
                                                    DV_QUANTITY matches {
                                                        property matches {[openehr::119]}
                                                        units matches {"mg/dL", "mmol/L"}
                                                        magnitude matches {|180.0..600.0|}
                                                    }
                                                }
                                            }
                                        }
