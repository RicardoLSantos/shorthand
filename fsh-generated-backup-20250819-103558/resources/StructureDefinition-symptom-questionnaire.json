{
  "resourceType": "StructureDefinition",
  "id": "symptom-questionnaire",
  "url": "https://github.com/RicardoLSantos/shorthand/StructureDefinition/symptom-questionnaire",
  "name": "SymptomQuestionnaire",
  "title": "Symptom Assessment Questionnaire",
  "status": "draft",
  "description": "Questionnaire template for symptom assessments",
  "fhirVersion": "4.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Questionnaire",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Questionnaire",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Questionnaire.title",
        "path": "Questionnaire.title",
        "mustSupport": true
      },
      {
        "id": "Questionnaire.status",
        "path": "Questionnaire.status",
        "mustSupport": true
      },
      {
        "id": "Questionnaire.item",
        "path": "Questionnaire.item",
        "slicing": {
          "discriminator": [
            {
              "type": "pattern",
              "path": "linkId"
            }
          ],
          "rules": "open"
        },
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Questionnaire.item:symptomType",
        "path": "Questionnaire.item",
        "sliceName": "symptomType",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Questionnaire.item:symptomType.linkId",
        "path": "Questionnaire.item.linkId",
        "patternString": "symptom-type"
      },
      {
        "id": "Questionnaire.item:symptomType.text",
        "path": "Questionnaire.item.text",
        "patternString": "Type of Symptom"
      },
      {
        "id": "Questionnaire.item:symptomType.type",
        "path": "Questionnaire.item.type",
        "patternCode": "choice"
      },
      {
        "id": "Questionnaire.item:symptomType.required",
        "path": "Questionnaire.item.required",
        "patternBoolean": true
      },
      {
        "id": "Questionnaire.item:location",
        "path": "Questionnaire.item",
        "sliceName": "location",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Questionnaire.item:location.linkId",
        "path": "Questionnaire.item.linkId",
        "patternString": "location"
      },
      {
        "id": "Questionnaire.item:location.text",
        "path": "Questionnaire.item.text",
        "patternString": "Location"
      },
      {
        "id": "Questionnaire.item:location.type",
        "path": "Questionnaire.item.type",
        "patternCode": "string"
      },
      {
        "id": "Questionnaire.item:onset",
        "path": "Questionnaire.item",
        "sliceName": "onset",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Questionnaire.item:onset.linkId",
        "path": "Questionnaire.item.linkId",
        "patternString": "onset"
      },
      {
        "id": "Questionnaire.item:onset.text",
        "path": "Questionnaire.item.text",
        "patternString": "When did it start?"
      },
      {
        "id": "Questionnaire.item:onset.type",
        "path": "Questionnaire.item.type",
        "patternCode": "dateTime"
      },
      {
        "id": "Questionnaire.item:description",
        "path": "Questionnaire.item",
        "sliceName": "description",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Questionnaire.item:description.linkId",
        "path": "Questionnaire.item.linkId",
        "patternString": "description"
      },
      {
        "id": "Questionnaire.item:description.text",
        "path": "Questionnaire.item.text",
        "patternString": "Description"
      },
      {
        "id": "Questionnaire.item:description.type",
        "path": "Questionnaire.item.type",
        "patternCode": "text"
      }
    ]
  }
}
