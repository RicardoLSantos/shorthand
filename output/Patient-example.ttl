# - resource -------------------------------------------------------------------

<https://2rdoc.pt/ig/ios-lifestyle-medicine/Patient/example> a fhir:Patient ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "example"] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p class=\"res-header-id\"><b>Narrativa gerada: Patient example</b></p><a name=\"example\"> </a><a name=\"hcexample\"> </a><p style=\"border: 1px #661aff solid; background-color: #e6e6ff; padding: 10px;\">Patient Example  Unknown, DoB: 1970-01-01 ( http://example.org/patients#example-patient)</p><hr/></div>"^^rdf:XMLLiteral
  ] ; # 
  fhir:identifier ( [
     fhir:system [ fhir:v "http://example.org/patients"^^xsd:anyURI ] ;
     fhir:value [ fhir:v "example-patient" ]
  ] ) ; # 
  fhir:name ( [
     fhir:family [ fhir:v "Example" ] ;
     fhir:given ( [ fhir:v "Patient" ] )
  ] ) ; # 
  fhir:gender [ fhir:v "unknown"] ; # 
  fhir:birthDate [ fhir:v "1970-01-01"^^xsd:date] . # 

