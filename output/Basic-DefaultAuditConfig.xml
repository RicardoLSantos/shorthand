<Basic xmlns="http://hl7.org/fhir">
  <id value="DefaultAuditConfig"/>
  <meta>
    <profile value="https://2rdoc.pt/ig/ios-lifestyle-medicine/StructureDefinition/mindfulness-audit-config"/>
  </meta>
  <text>
    <status value="extensions"/><div xmlns="http://www.w3.org/1999/xhtml"><p class="res-header-id"><b>Narrativa gerada: Basic DefaultAuditConfig</b></p><a name="DefaultAuditConfig"> </a><a name="hcDefaultAuditConfig"> </a><div style="display: inline-block; background-color: #d9e0e7; padding: 6px; margin: 4px; border: 1px solid #8da1b4; border-radius: 5px; line-height: 60%"><p style="margin-bottom: 0px"/><p style="margin-bottom: 0px">Perfil: <a href="StructureDefinition-mindfulness-audit-config.html">Mindfulness Audit Configuration</a></p></div><p><b>Mindfulness Audit Level</b>: detailed</p><p><b>Mindfulness Audit Retention</b>: Nenhum ecrã para Duration  (value: 90; system: http://unitsofmeasure.org; code: days)</p><p><b>Mindfulness Audit Format</b>: fhir</p><p><b>code</b>: <span title="Códigos:{https://2rdoc.pt/fhir/CodeSystem/mindfulness-config-type audit}">Audit Configuration</span></p></div>
  </text>
  <extension url="https://2rdoc.pt/ig/ios-lifestyle-medicine/StructureDefinition/mindfulness-audit-level">
    <valueCode value="detailed"/>
  </extension>
  <extension url="https://2rdoc.pt/ig/ios-lifestyle-medicine/StructureDefinition/mindfulness-audit-retention">
    <valueDuration>
      <value value="90"/>
      <system value="http://unitsofmeasure.org"/>
      <code value="days"/>
    </valueDuration>
  </extension>
  <extension url="https://2rdoc.pt/ig/ios-lifestyle-medicine/StructureDefinition/mindfulness-audit-format">
    <valueCode value="fhir"/>
  </extension>
  <code>
    <coding>
      <system value="https://2rdoc.pt/fhir/CodeSystem/mindfulness-config-type"/>
      <code value="audit"/>
      <display value="Audit Configuration"/>
    </coding>
  </code>
</Basic>